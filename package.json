    "@angular/forms": "5.0.5",
    "@angular/http": "5.0.5",
    "@angular/platform-browser": "5.0.5",
    "@angular/platform-browser-dynamic": "5.0.5",
    "@angular/router": "5.0.5",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "4.1.1",
    "@ngrx/store": "4.1.1",
    "@ngrx/store-devtools": "4.1.1",
    "chart.js": "2.7.1",
    "core-js": "2.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.2",
    "zone.js": "0.8.18"
  },
