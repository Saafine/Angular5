    "node-sass": "4.7.2",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "2.0.9",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "to-string-loader": "1.1.5",
