{
  "name": "xkantor-backoffice",
  "version": "1.0.0",
  "description": "app",
  "scripts": {
    "start": "set NODE_ENV=dev && node node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress",
    "build:dev": "set NODE_ENV=dev && npm run clean:dist && node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --profile",
    "build:prod": "set NODE_ENV=prod && npm run clean:dist && node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --profile --bail",
    "clean:dist": "rimraf -- dist",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:fix": "tslint ./src/**/*.ts --fix"
  },
  "dependencies": {
    "@angular/common": "5.0.5",
    "@angular/compiler": "5.0.5",
